{
  "name": "@onyx/db",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./schema": "./src/schema/index.ts"
  },
  "scripts": {
    "generate": "drizzle-kit generate",
    "migrate": "bun --env-file=../../.env drizzle-kit migrate",
